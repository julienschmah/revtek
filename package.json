    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "postcss": "^8.5.3",
    "sequelize-cli": "^6.6.3",
    "tailwindcss": "^4.1.6"
  },
  "dependencies": {
    "axios-cache-adapter": "^2.7.3",
    "multer": "^2.0.0",
    "node-cache": "^5.1.2",
    "react-input-mask": "^2.0.4"
  }
}
